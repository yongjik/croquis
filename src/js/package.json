{
    "name": "croquis-js",
    "version": "0.2.0",
    "description": "Croquis - the js part",
    "main": "lib/index.js",
    "author": "Yongjik Kim",
    "license": "MIT",
    "style": "style/croquis_fe.css",
    "scripts": {
        "build": "npm run build:lib && npm run build:labextension:dev",
        "build:prod": "npm run clean && npm run build:lib:prod && npm run build:labextension",
        "build:labextension": "jupyter labextension build .",
        "build:labextension:dev": "jupyter labextension build --development True .",
        "build:lib": "tsc --sourceMap",
        "build:lib:prod": "tsc",
        "clean": "npm run clean:lib && npm run clean:labextension && npm run clean:lintcache",
        "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
        "clean:lintcache": "rimraf .eslintcache",
        "clean:labextension": "rimraf ../myjext/labextension",
        "eslint": "eslint . --cache"
    },
    "dependencies": {
        "@jupyterlab/application": "^4.2.3",
        "@jupyterlab/notebook": "^4.2.3",
        "@jupyterlab/rendermime": "^4.2.3",
        "@jupyterlab/rendermime-interfaces": "^3.10.3",
        "@lumino/coreutils": "^2.1.2",
        "@lumino/widgets": "^2.4.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.16.0",
        "@jupyterlab/builder": "^4.2.3",
        "eslint": "^8.57.1",
        "globals": "^15.12.0",
        "rimraf": "^5.0.1",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.26.0"
    },
    "jupyterlab": {
        "extension": true,
        "outputDir": "../myjext/labextension"
    }
}
